    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'vcd_rust'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=vcd_rust"
                ],
                "filter": {
                    "name": "vcd_rust",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug integration test 'file_parsing_integration_test'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--test=file_parsing_integration_test",
                    "--package=vcd_rust"
                ],
                "filter": {
                    "name": "file_parsing_integration_test",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}